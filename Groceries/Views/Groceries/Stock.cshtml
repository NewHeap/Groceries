@model GrocerieViewModel
@model List<Groceries>
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Stock</title>
</head>
<body>

<h4>Stock</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <table class="table table-striped">
            <thead>
                <tr>
                    <td>Id</td>
                    <td>Product</td>
                    <td>Stock</td>
                </tr>
            </thead>
            @foreach (var item in Model)
    {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Product</td>
                    <td>@item.Stock</td>
                </tr>
    }
        </table>
        <div class="form-group">
            <input type="submit" value="Save" class="btn btn-default" />
       </div>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

</body>
</html>


